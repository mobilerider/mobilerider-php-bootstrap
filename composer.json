{
    "name": "mobilerider/mobilerider-php-bootstrap",
    "type": "library",
    "description": "Base classes and functions for MobileRider source code",
    "version": "0.0.1-alpha.10",
    "keywords": ["mobilerider"],
    "homepage": "http://www.mobilerider.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Perez",
            "email": "michel@mobilerider.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mr\\Bootstrap\\": "src/",
            "Mr\\BootstrapTests\\": "tests/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Akamai\\AnalyticsTest\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "rappasoft/laravel-helpers": "^1.0",
        "psr/log": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "guzzlehttp/promises": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
